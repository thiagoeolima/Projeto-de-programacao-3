package br.ufal.ic.comp215.srm;

import java.util.Date;

public class Resources {
	
	private int id;
	private Date initialDate;
	private Date endDate;
	private String status;
	private User resp;
	private Activities activ;
	private Place place;
	
	
	/**
	 * Product constructor
	 * 
	 * @param id The resource's id.
	 * @param initialDate The Inicial Date of resource
	 * @param endDate The End Date of resource
	 * @param status The plugs type at the place
	 * @param resp The user responsible for the resource
	 * @param activ The activities for the resource
	 * @param place The place of resource
	 * 
	 */
	
	public Resources(int id, Date initialDate, Date endDate, String status,
			User resp, Activities activ, Place place) {
		super();
		this.id = id;
		this.initialDate = initialDate;
		this.endDate = endDate;
		this.status = status;
		this.resp = resp;
		this.activ = activ;
		this.place = place;
	}

}
